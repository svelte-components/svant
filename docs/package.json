{
  "name": "svant",
  "description": "Svelte componet library based on the Ant Design System",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn gen:code-text && sapper dev",
    "build": "yarn gen:code-text && sapper build --legacy",
    "gen:code-text": "node scripts/generateCodeText run",
    "export": "sapper export --legacy",
    "start": "yarn gen:code-text && node __sapper__/build",
