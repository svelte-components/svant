{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn generate:icons && rollup -c",
    "dev": "cd docs && yarn dev",
    "generate:icons": "node scripts/generateIcons.js ",
    "start": "sirv public",
    "lint": "prettier --check '**/*.js' '**/*.svelte' &>/dev/null || (echo \"Linting failed. Run yarn lint:fix to fix the formatting issues\" &&  exit 1)",
    "lint:fix": "prettier --write '**/*.js' '**/*.svelte'",
    "create:component": "node scripts/createComponentRoute.js",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "husky": "^4.2.5",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.3.0",
    "flush-promises": "^1.0.2",
    "jest": "^25.3.0",
    "jest-transform-svelte": "^2.1.1",
    "less": "^3.11.1",
    "postcss": "^7.0.27",
    "prettier": "^1.16.4",
    "prettier-plugin-svelte": "^0.7.0",
    "prompts": "^2.3.2",
    "rollup": "^2.3.2",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.7.1"
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.0",
    "@ant-design/icons-svg": "^4.0.0",
    "classnames": "^2.2.6",
    "css-animation": "^1.5.0",
    "lodash": "^4.17.15",
    "raf": "^3.4.1",
    "sirv-cli": "^0.4.5",
    "style-to-object": "^0.3.0",
    "to-style": "^1.3.3"
  }
}
